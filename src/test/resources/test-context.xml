<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">

	<util:list id="managers">
		<bean class="sk.seges.test.jms.manager.EmbeddedActiveMQJMSProviderManager"/>
		<bean class="sk.seges.test.jms.manager.OpenMQJMSProviderManager"/>
	</util:list>	

	<util:list id="jmsSuiteTestManagers">
		<ref local="reportingProviderManager"/>
		<ref local="reportingProviderManager"/>
	</util:list>
	
	<bean id="reportingProviderManager" class="sk.seges.test.jms.mock.ReportingJMSProviderManager" scope="prototype" />
	
</beans>